{
  "manifest_version": 2,
  "name": "calm.io",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["p5.min.js", "sketch.js"]
    }
  ],
  "background": {
    "scripts":["background.js"]
   },
   "browser_action": {
     "default_icon": "waves.png",
     "default_popup": "index.html",
     "default_title": "A popup will come here"
 }
}
